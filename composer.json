{
    "name": "kairos/cacheablebundle",
    "type": "symfony-bundle",
    "description": "A simple cache bundle for Symfony2",
    "keywords": ["symfony"],
    "homepage": "https://github.com/t0k4rt/CacheableBundle",
    "license": "MPL-2.0",
    "authors": [{
        "name": "Henrik Westphal",
        "email": "henrik.westphal@gmail.com"
    },{
        "name": "Community contributors",
        "homepage": "https://github.com/snc/SncRedisBundle/contributors"
    }],
    "require": {
        "php": ">=5.3.3",
        "jms/metadata": "~1.1",
        "symfony/yaml": "~2.1",
        "doctrine/annotations": "~1.1",
        "doctrine/cache": "~1.3",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3.0"
    },
    "suggest": {
        "doctrine/doctrine-cache-bundle": "Use all the cache providers you want that are provided by doctrine cache."
    },
    "autoload": {
        "psr-0": { "Kairos\\CacheableBundle": "" }
    },
    "target-dir": "Kairos/CacheableBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
